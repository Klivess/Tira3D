#shader vertex
#version 330 core
#extension GL_ARB_separate_shader_objects : enable

layout(location = 0) in vec2 position;

void main() {
    gl_Position = vec4(position);
}
#shader fragment
#version 330 core


layout(location = 0) out vec4 color;

void main() {
    color = vec4(0.0, 0.5, 0.5, 0);
}